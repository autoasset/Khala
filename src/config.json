{
    "inputs": [
        "./assets/icons"
    ],
    "exclude": [
        "./assets/icons/dui_analysis_icon.svg"
    ],
    "outputs": {
        "gif2x": "./coverter/gif2x",
        "gif3x": "./coverter/gif3x",
        "icon2x": "./coverter/icon2x",
        "icon3x": "./coverter/icon3x",
        "svg2pdf": "./coverter/svg2pdf",
        "svg2fonts": [
            {
                "fontname": "custom",
                "svgfont": "./coverter/svg2fonts/custom.svg",
                "ttf": "./coverter/svg2fonts/custom.ttf"
            }
        ],
        "svg2xml": "./coverter/svg2xml",
        "pdf": "./coverter/pdf",
        "svg": "./coverter/svg",
        "other": "./coverter/other"
    },
    "products": {
        "ios": {
            "vector_template": "./products/ios/vector_template",
            "fonts": [
                {
                    "fontname": "custom",
                    "path": "./products/ios/iconfont"
                }
            ],
            "icon": "./products/ios/icon",
            "gif": "./products/ios/gif"
        },
        "android": {
            "build_settings": {
                "copy_2x_inputs": [
                    "./assets/patch9_2x"
                ],
                "copy_3x_inputs": [
                    "./assets/patch9_3x"
                ]
            },
            "vector_template": "./products/android/vector_template",
            "x2": "./products/android/x2",
            "x3": "./products/android/x3"
        },
        "flutter": {
            "fonts": [
                {
                    "fontname": "iconfont",
                    "path": "./products/flutter/iconfont"
                }
            ]
        }
    }
}